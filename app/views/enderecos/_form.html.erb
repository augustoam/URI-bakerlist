<%= simple_form_for(@endereco) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :endereco %>

    <%= f.input :numero %>

    <%= f.input :referencia %>

    <%= f.input :obs %>

    <%= f.input :cep %>

    <%= f.input :cidade_id %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
